{
    "name": "bradietilley/laravel-achievements",
    "description": "Log changes to your models in your Laravel application",
    "keywords": [
        "laravel",
        "reflection"
    ],
    "homepage": "https://github.com/bradietilley/laravel-achievements",
    "license": "MIT",
    "authors": [
        {
            "name": "Bradie Tilley",
            "homepage": "https://github.com/bradietilley",
            "email": "bradietilley@gmail.com"
        }
    ],
    "require": {
        "laravel/framework": "^11.0",
        "spatie/laravel-package-tools": "^1.16"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "pestphp/pest": "^2.0",
        "phpstan/phpstan": "^1.8",
        "orchestra/testbench": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "BradieTilley\\Achievements\\": "src/",
            "BradieTilley\\Achievements\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BradieTilley\\Achievements\\AchievementsServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
